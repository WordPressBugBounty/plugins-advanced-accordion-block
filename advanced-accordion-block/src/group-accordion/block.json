		"aagb/accordion-readMoreText" : "readText",
		"aagb/accordion-border" : "border",
		"aagb/accordion-margins" : "margins",
		"aagb/accordion-paddings" : "paddings",
		"aagb/accordion-borderRadius" : "borderRadius",
		"aagb/accordion-headingColor" : "headingColor",
		"aagb/accordion-showIcon" : "showIcon",
		"aagb/accordion-iconColor" : "iconColor",
