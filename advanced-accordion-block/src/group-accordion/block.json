		"stepCmpltText": {
			"type": "string",
			"default": "Congratulations, you are done!"
		},
		"checkList": {
			"type": "boolean",
			"default": false
		},
		"buttonShow":{
			"type": "boolean",
			"default": false
		},
		"contentCount": {
			"type": "number",
			"default": 300
		},
		"readText":{
			"type": "string",
			"default": "Read More"
		},
		"border": {
			"type": "object",
			"default": {
				"width": "1px",
				"color": "#cccccc",
				"style": "solid"
			}
		},
		"paddings": {
			"type": "object",
			"default": {
				"top": "10px",
				"left": "15px",
				"right": "15px",
				"bottom": "10px"
			}
		},
		"margins": {
			"type": "object",
			"default": {
				"top": "0px",
				"bottom": "15px"
			}
		},
		"borderRadius": {
			"type": "number",
			"default": 0
		},
		"headingColor": {
			"type": "string"
		},
		"headerBg": {
			"type": "string"
		},"iconColor": {
			"type": "string"
		},
		"iconBackground": {
			"type": "string"
		},
		"bodyBg": {
			"type": "string"
		},"showIcon": {
			"type": "boolean",
			"default": true
		},
		"qIconText": {
			"type": "string",
			"default": "Q"
		},
		"qIconColor": {
			"type": "string",
			"default": "#fff"
		},
		"qIconBg": {
			"type": "string",
			"default": "#505050"
		},
		"aIconText": {
			"type": "string",
			"default": "A"
		},
		"aIconColor": {
			"type": "string",
			"default": "#fff"
		},
		"aIconBg": {
			"type": "string",
			"default": "#f5a623"
		},
		"customCSS": {
			"type": "string",
			"default": ""
		}
	},
	"styles": [
		{
			"name": "default",
			"label": "Default",
			"isDefault": true
		},
		{
			"name": "qa",
			"label": "Q A"
		},
		{
			"name": "aab-style-pro-checked",
			"label": "aab-style-pro-checked"
		}
	],
	"textdomain": "advanced-accordion-block",
	"editorScript": [
		"file:./index.js",
		"aagb-anchor",
		"aagb-separate-accordion",
		"aagb-accordion-group"
	],
	"editorStyle": "file:./index.css",
	"style": [
		"aagb-group-accordion"
	],
	"viewScript": [
		"aagb-anchor",
		"aagb-separate-accordion",
		"aagb-accordion-group",
		"aagb-group-accordion-frontend"
	]
}
